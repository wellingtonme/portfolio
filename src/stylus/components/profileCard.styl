@import '../tools/variables.styl'

.parallax
	position absolute
	top 0
	right 0
	left 0
	background-image url('https://c2.staticflickr.com/8/7331/9262740777_f69bca112c_b.jpg')
	background-size cover
	background-position-y center
	&::before
		background-color rgba(0, 0, 0, 0.8)
		content ''
		top 0
		left 0
		width 100%
		height 100%
		position absolute

.presentation-card
	margin-top 200px

.profile
	&--contact-me
		margin-left 30px
